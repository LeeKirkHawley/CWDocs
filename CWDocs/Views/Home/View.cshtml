@using CWDocs.Models
@model CWDocsViewModel
@{
    ViewData["Title"] = "View Document";
    Layout = "_CWDocsLayout";
}

<input asp-for="documentId" type="hidden" />

<strong>Name: </strong> @Model.originalDocumentName
<strong>File: </strong>@Model.documentName
<strong>Date: </strong>@Model.documentDate

<img class="img" id="image" src="@Model.image" style="max-width:600px" />

